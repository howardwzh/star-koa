{
  "name": "vue-blog",
  "version": "1.0.0",
  "license": "MIT",
  "description": "a blog platform built with vue2, koa2 and mongo",
  "scripts": {
    "dev": "nodemon server/start.js",
    "start": "npm run build && npm run prod",
    "prod": "cross-env NODE_ENV=production node server/start.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --colors --profile --progress",
    "build:json": "cross-env NODE_ENV=production webpack --config webpack.config.js --colors --profile --display-modules --sort-modules-by size --json > stats.json",
    "build:detail": "cross-env NODE_ENV=production webpack --config webpack.config.js --colors --profile --display-modules --sort-modules-by size",
    "pm2": "npm run build && pm2 start pm2.json --env production"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "element-ui": "^1.2.7",
    "fastclick": "^1.0.6",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.10.0",
    "import-dir": "^0.0.1",
    "koa": "^2.2.0",
    "koa-bodyparser": "1",
    "koa-connect-history-api-fallback": "^0.3.1",
    "koa-convert": "^1.2.0",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^3.1.0",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "marked": "^0.3.6",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "mongoose": "^4.9.3",
    "simplemde": "^1.11.2",
    "vue": "^2.2.6",
    "vue-router": "^2.3.1",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-component": "^0.9.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "clean-webpack-plugin": "^0.1.16",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "*",
    "html-webpack-plugin": "^2.28.0",
    "jsonwebtoken": "^7.3.0",
    "koa-webpack": "^0.3.1",
    "koa2-webpack-dev-middleware": "^0.0.1",
    "nodemon": "^1.11.0",
    "style-loader": "^0.16.1",
    "stylus": "0.52.4",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.0.0",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  }
}
